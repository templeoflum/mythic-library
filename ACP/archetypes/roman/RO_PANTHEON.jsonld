    {
      "@id": "system:roman",
      "@type": "System",
      "systemCode": "RO",
      "name": "Roman Pantheon – Dii Consentes",
      "description": "The gods of the Roman state religion, adapted from Greek models but fundamentally reshaped by Roman civic values. Roman deities emphasize institutional order, civic duty, agricultural fertility, and ancestral authority over the personal drama and moral ambiguity of their Greek counterparts.",
      "culturalContext": "Roman Republic and Empire, ~753 BCE - 476 CE",
      "primarySources": ["Ovid's Metamorphoses", "Virgil's Aeneid", "Livy's Ab Urbe Condita", "Cicero's De Natura Deorum", "Varro's Antiquitates Rerum Divinarum"],
      "characteristics": [
        "Civic religion - gods serve the state and its people",
        "Pietas (duty to gods, state, family) as central virtue",
        "Interpretatio romana absorbed foreign gods into Roman framework",
        "Ritual precision and contractual relationship with deities (do ut des)",
        "Ancestral tradition (mos maiorum) shapes divine character"
      ],
      "cosmology": {
        "capitalTemples": ["Jupiter Optimus Maximus (Capitoline Hill)", "Mars Ultor (Forum of Augustus)", "Venus Genetrix (Forum of Caesar)"],
        "priesthoods": ["Pontifex Maximus", "Augures", "Flamines", "Vestal Virgins"],
        "festivals": ["Saturnalia", "Lupercalia", "Parentalia", "Ludi Romani"]
      }
    },

