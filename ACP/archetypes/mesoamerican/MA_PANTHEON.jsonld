{
  "@context": "../../schema/ACP_CONTEXT.jsonld",
  "@id": "pantheon:MA-PANTHEON",
  "@type": "Pantheon",
  "name": "Mesoamerican Deities",
  "tradition": "Aztec (Mexica) / Maya / Toltec / Olmec",
  "description": "The gods and divine forces of Mesoamerican civilizations, primarily the Aztec (Mexica) and Maya traditions. These traditions share deep roots while developing distinct expressions, unified by concepts of cosmic cycles, blood sacrifice, duality, and the intimate relationship between gods and humanity through mutual sustenance.",
  "canonicalSources": [
    "Florentine Codex (Bernardino de Sahagún)",
    "Popol Vuh (K'iche' Maya)",
    "Codex Borgia",
    "Codex Dresden (Maya)",
    "Historia de las Indias de Nueva España",
    "Leyenda de los Soles"
  ],
  "systemPatterns": [
    {
      "name": "Five Suns Cosmology",
      "description": "The universe has been created and destroyed four times; we live in the Fifth Sun, destined to end in earthquakes. Each sun required divine sacrifice to begin.",
      "archetypes": [
        "arch:MA-QUETZALCOATL",
        "arch:MA-TEZCATLIPOCA",
        "arch:MA-TONATIUH"
      ],
      "pattern": "Cyclical creation-destruction requiring sacrifice"
    },
    {
      "name": "Tezcatlipoca-Quetzalcoatl Duality",
      "description": "The eternal opposition and collaboration of the Smoking Mirror and the Feathered Serpent, representing darkness/light, sorcery/civilization, chaos/order",
      "archetypes": [
        "arch:MA-TEZCATLIPOCA",
        "arch:MA-QUETZALCOATL"
      ],
      "pattern": "Necessary opposition creating cosmic balance"
    },
    {
      "name": "Hero Twins (Maya)",
      "description": "Hunahpú and Xbalanqué who defeated the Lords of Xibalba through cleverness and sacrifice, becoming sun and moon",
      "archetypes": [
        "arch:MA-HERO-TWINS"
      ],
      "pattern": "Trickster heroes overcoming death through wit"
    },
    {
      "name": "Divine Sacrifice",
      "description": "Gods sacrificing themselves to create and sustain the cosmos, requiring human reciprocation through blood and heart offerings",
      "archetypes": [
        "arch:MA-QUETZALCOATL",
        "arch:MA-NANAHUATZIN"
      ],
      "pattern": "Mutual sustenance between gods and humanity"
    },
    {
      "name": "Tlaloc Complex",
      "description": "The rain god and associated water/fertility deities controlling agriculture and requiring child sacrifice",
      "archetypes": [
        "arch:MA-TLALOC",
        "arch:MA-CHALCHIUHTLICUE"
      ],
      "pattern": "Fertility through sacrifice to water powers"
    }
  ],
