  "systemPatterns": [
    {
      "name": "Separation of Rangi and Papa",
      "description": "The primordial parents (Sky Father and Earth Mother) held in eternal embrace until their children force them apart to create space for life",
      "archetypes": ["arch:PL-RANGI", "arch:PL-PAPA", "arch:PL-TANE"],
      "pattern": "Creation through separation of primordial unity"
    },
    {
      "name": "Maui's Deeds",
      "description": "The trickster hero who fished up islands, snared the sun, stole fire, and died attempting to win immortality for humanity",
      "archetypes": ["arch:PL-MAUI"],
      "pattern": "Culture hero's transformative feats and noble death"
    },
    {
      "name": "Pele and Hi'iaka",
      "description": "The volcanic goddess and her loyal sister's epic journey, exploring themes of jealousy, devotion, destruction, and reconciliation",
      "archetypes": ["arch:PL-PELE", "arch:PL-HIIAKA"],
      "pattern": "Divine sisterhood, jealousy, and volcanic transformation"
    },
    {
      "name": "The Four Great Gods",
      "description": "Kū, Kāne, Lono, and Kanaloa - the four major male deities of Hawaiian tradition governing war, life, fertility, and ocean/death",
      "archetypes": ["arch:PL-KU", "arch:PL-KANE", "arch:PL-LONO", "arch:PL-KANALOA"],
      "pattern": "Quadripartite divine masculine structure"
    },
    {
      "name": "Mana and Tapu",
      "description": "The flow of sacred power (mana) and its regulation through sacred restrictions (tapu/kapu), governing all divine-human interaction",
      "archetypes": ["All"],
      "pattern": "Sacred power dynamics and cosmic regulation"
    }
  ],
