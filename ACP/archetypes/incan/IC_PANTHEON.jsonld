{
  "@context": "../../schema/ACP_CONTEXT.jsonld",
  "@id": "pantheon:incan",
  "@type": "Pantheon",
  "name": "Incan & Andean Pantheon",
  "tradition": "Incan/Pre-Columbian Andean",
  "description": "The divine beings of the Inca Empire and broader Andean traditions, centered on Inti (Sun) worship, the cosmic significance of Viracocha as creator, and the animate landscape of huacas (sacred places). The Sapa Inca ruled as Son of the Sun, mediating between cosmic and human realms. Andean cosmology emphasizes reciprocity (ayni), complementary duality (yanantin), and the three-world structure of Hanan Pacha (upper world), Kay Pacha (this world), and Ukhu Pacha (inner/lower world).",
  "canonicalSources": [
    "Spanish Colonial Chronicles (Garcilaso de la Vega, Cieza de León, Guaman Poma)",
    "Huarochirí Manuscript (c. 1598)",
    "Archaeological Evidence (Machu Picchu, Coricancha, Nazca)",
    "Oral Traditions of Quechua and Aymara Peoples"
  ],
  "systemPatterns": [
    {
      "name": "Viracocha Creation",
      "description": "Viracocha emerges from Lake Titicaca, creates sun, moon, and stars, then fashions humanity from stone, breathing life into them. After teaching civilization, he walks across the water westward, promising to return.",
      "archetypes": [
        "incan:viracocha",
        "incan:inti",
        "incan:mama-quilla"
      ]
    },
    {
      "name": "Inti and the Sapa Inca",
      "description": "The Sun God Inti is father to the Inca dynasty. The Sapa Inca rules as Son of the Sun, performing rituals at Inti Raymi to ensure cosmic order and agricultural abundance.",
      "archetypes": [
        "incan:inti",
        "incan:mama-ocllo",
        "incan:manco-capac"
      ]
    },
    {
      "name": "Pachamama and Reciprocity",
      "description": "Pachamama as living Earth requires offerings and respect in exchange for fertility and sustenance. The principle of ayni (reciprocity) governs all relationships—human, divine, and ecological.",
      "archetypes": [
        "incan:pachamama",
        "incan:mama-cocha",
        "incan:illapa"
      ]
    },
    {
      "name": "Three Worlds Cosmology",
      "description": "The cosmos divided into Hanan Pacha (celestial realm of gods), Kay Pacha (earthly realm of humans), and Ukhu Pacha (inner world of ancestors and origins). The condor, puma, and serpent symbolize these realms respectively.",
      "archetypes": [
        "incan:viracocha",
        "incan:supay",
        "incan:pachamama"
      ]
    }
  ],
