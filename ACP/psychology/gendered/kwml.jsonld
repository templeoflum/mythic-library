  "name": "King, Warrior, Magician, Lover (KWML)",
  "description": "Moore and Gillette's model of mature masculine archetypes and their shadow forms. Each archetype has bipolar shadow: active (inflated) and passive (deflated).",
  "domain": "psychology",
  "subdomain": "gendered",
  "cardinality": 8,
  "structuralNotes": "Based on Robert Moore and Douglas Gillette's 'King, Warrior, Magician, Lover' (1990). Each archetype has fullness form and two shadow poles.",

  "entries": [
    {
      "@id": "kwml:king",
      "@type": "Archetype",
      "name": "The King",
      "keywords": ["sovereignty", "blessing", "order", "fertility", "centeredness"],
      "description": "The energy of rightful authority and creative ordering. The King blesses, creates order, and ensures the fertility of the realm. Centered, calm, defines reality for others.",
      "spectralCoordinates": {
        "order-chaos": 0.15,
        "creation-destruction": 0.35,
        "light-shadow": 0.25,
        "active-receptive": 0.40,
        "individual-collective": 0.70,
        "ascent-descent": 0.30,
        "stasis-transformation": 0.30,
        "voluntary-fated": 0.50
      },
      "instantiates": [
        {"primordial": "primordial:sovereign", "weight": 0.95, "aspects": ["blessing", "order", "centeredness"]},
        {"primordial": "primordial:great-father", "weight": 0.70, "aspects": ["fertility", "authority"]}
      ],
      "relationships": [
        {"type": "SHADOW", "target": "kwml:tyrant", "notes": "Active shadow"},
        {"type": "SHADOW", "target": "kwml:weakling", "notes": "Passive shadow"},
        {"type": "CULTURAL_ECHO", "target": "arch:GR-ZEUS", "fidelity": 0.75, "note": "King of gods"}
      ]
    },
    {
