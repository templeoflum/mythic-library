{
  "@context": "../../schema/ACP_CONTEXT.jsonld",
  "@id": "pantheon:persian",
  "@type": "Pantheon",
  "name": "Persian & Zoroastrian Pantheon",
  "tradition": "Persian/Zoroastrian/Iranian",
  "description": "The divine beings of ancient Iranian religion, culminating in the Zoroastrian reform of Zarathustra. This tradition centers on the cosmic dualism between Ahura Mazda (Wise Lord) and Angra Mainyu (Destructive Spirit), with humanity's role in the cosmic battle between Asha (Truth/Order) and Druj (Lie/Chaos). The Yazatas (worthy of worship) serve as divine helpers, while the Amesha Spentas (Bounteous Immortals) represent aspects of Ahura Mazda. This dualistic framework profoundly influenced later Abrahamic concepts of good vs. evil, angels, heaven, hell, and apocalyptic eschatology.",
  "canonicalSources": [
    "Avesta (Yasna, Yasht, Vendidad)",
    "Bundahishn (Creation text)",
    "Denkard (Acts of Religion)",
    "Shahnameh (Ferdowsi)",
    "Greek sources (Herodotus, Plutarch)"
  ],
  "systemPatterns": [
    {
      "name": "Cosmic Dualism",
      "description": "The fundamental opposition between Ahura Mazda (Truth, Light, Life) and Angra Mainyu (Lie, Darkness, Death). Creation is the battleground where these forces contend, with eventual victory for good predetermined but requiring human participation.",
      "archetypes": ["persian:ahura-mazda", "persian:angra-mainyu"]
    },
    {
      "name": "Amesha Spentas",
      "description": "The seven Bounteous Immortals who emanate from Ahura Mazda, each governing an aspect of creation and representing a divine virtue. They form a heptad of cosmic principles.",
      "archetypes": ["persian:ahura-mazda", "persian:spenta-mainyu", "persian:vohu-manah", "persian:asha-vahishta", "persian:khshathra-vairya", "persian:spenta-armaiti", "persian:haurvatat", "persian:ameretat"]
    },
    {
      "name": "Fravashi and Ancestors",
      "description": "The Fravashi are pre-existent souls/guardian spirits that chose to incarnate and fight for good. They connect living humans to ancestors and to cosmic purpose.",
      "archetypes": ["persian:fravashi"]
    },
    {
      "name": "Eschatological Battle",
      "description": "History moves toward Frashokereti (Making Wonderful), the final renovation when evil is defeated, the dead resurrected, and creation restored to perfection through the Saoshyant (savior).",
      "archetypes": ["persian:ahura-mazda", "persian:saoshyant", "persian:angra-mainyu"]
    }
  ],
